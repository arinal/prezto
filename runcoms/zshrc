# Source Prezto.
if [[ -s "${ZDOTDIR:-$HOME}/.zprezto/init.zsh" ]]; then
  source "${ZDOTDIR:-$HOME}/.zprezto/init.zsh"
fi

# Customize to your needs...

alias -g P="| "
alias -g PP="pbpaste | "
alias -g C="| pbcopy"
alias -g N="| /dev/null"
alias -g G='| grep'
alias -g L='| bat'
alias -g J='| jq'
alias -g T="| column -t -s $'\t'"

alias tm='tmux'
alias em='emacs -nw'
alias emc='emacsclient -nw'
alias e.='e .'
alias f=ranger

alias gtd='emacsclient -nw /home/archinal/Dropbox/Documents/orgs/TODO.org'

source awsp_functions.sh
